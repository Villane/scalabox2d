			</TopRightPixmap>
			
			<RightEdgePixmap x="8" y="17" width="2" height="2"> <!-- right hand side edge -->
				<Texture ref="QtCurve"/>
			</RightEdgePixmap>
			
			<BottomRightPixmap x="8" y="17" width="2" height="0"> <!-- bottom-right corner -->
